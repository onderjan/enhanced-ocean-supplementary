                RowBox[{"a", "-", "1"}], ")"}], "*", "k"}], "+", 
              RowBox[{"1", "/", "2"}]}], "]"}], "+", "1"}], ")"}]}], "+", 
         "\[Phi]"}], "]"}], "+", 
       RowBox[{"Cos", "[", 
        RowBox[{
         RowBox[{"2", "\[Pi]", "*", 
          FractionBox["n", "vN"], "*", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"Floor", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"(", 
                RowBox[{"a", "+", "1"}], ")"}], "*", "k"}], "+", 
              RowBox[{"1", "/", "2"}]}], "]"}], "-", "1"}], ")"}]}], "+", 
         "\[Phi]"}], "]"}]}], ")"}], "/", 
     RowBox[{"Cos", "[", 
      RowBox[{
       RowBox[{"2", " ", "\[Pi]", "*", 
        FractionBox["n", "vN"], "*", 
