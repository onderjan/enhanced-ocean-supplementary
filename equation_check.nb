  3.804267193457858*^9}},ExpressionUUID->"cc44604a-6327-4b4e-9df4-\
c3025319ef1f"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"oceanAlgorithmRules", "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"g", "[", "a_", "]"}], "\[Rule]", 
       RowBox[{"Floor", "[", 
        RowBox[{
         RowBox[{"a", "*", "k"}], "+", 
         RowBox[{"1", "/", "2"}]}], "]"}]}], ",", 
      RowBox[{
       RowBox[{"u", "[", "\[Phi]_", "]"}], "\[Rule]", "\[Phi]"}]}], "}"}]}], 
   ";"}], "\n"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"actualOceanOutput", "=", 
   RowBox[{"basicAlgorithmOutput", "/.", "oceanAlgorithmRules"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"expectedOutput", " ", "=", " ", 
   RowBox[{"A", "*", 
    RowBox[{"Cos", "[", 
     RowBox[{
      FractionBox[
       RowBox[{"2", " ", "n", " ", "\[Pi]", " ", 
        RowBox[{"g", "[", "a", "]"}]}], "vN"], "+", 
      RowBox[{"u", "[", "\[Phi]", "]"}]}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"expectedOceanOutput", "=", 
   RowBox[{"expectedOutput", "/.", "oceanAlgorithmRules"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"oceanModulationCurve", "=", 
   RowBox[{"actualOceanOutput", "/", "expectedOceanOutput"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"oceanModulationCurvePaper", "=", 
   RowBox[{
    FractionBox["vO", "16"], "*", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"4", "*", 
        RowBox[{"Cos", "[", 
         RowBox[{
          RowBox[{"2", "\[Pi]", "*", 
           FractionBox["n", "vN"], "*", 
           RowBox[{"Floor", "[", 
            RowBox[{
             RowBox[{"a", "*", "k"}], "+", 
             RowBox[{"1", "/", "2"}]}], "]"}]}], "+", "\[Phi]"}], "]"}]}], 
       "+", 
       RowBox[{"Cos", "[", 
        RowBox[{
         RowBox[{"2", "\[Pi]", "*", 
          FractionBox["n", "vN"], "*", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"Floor", "[", 
             RowBox[{
              RowBox[{
